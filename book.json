{
    "plugins": [
        "theme-api",
        "header",
        "sharing",
        "fontsettings",
        "toggle-chapters",
        "custom-favicon",
        "customJquery@git+https://github.com/dtolb/gitbook-plugin-customJquery.git"
    ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "csharp",
                    "name": "C#"
                },
                {
                    "lang": "ruby",
                    "name": "ruby"

                },
                {
                    "lang": "shell",
                    "name": "cURL"
                }
            ]
        },
      "customJquery": {
        "js": "js/custom.js"
      },
        "layout": {
            "headerPath" : "layouts/header.html"
        },
        "favicon": "images/favicon.ico",
        "sharing": {
            "facebook": false,
            "twitter": false,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        }
    }
}
